<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="CronoJpa" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>DatoClases.Asistencia</class>
        <class>DatoClases.Chequeo</class>
        <class>DatoClases.Deportes</class>
        <class>DatoClases.Deportista</class>
        <class>DatoClases.Entrenador</class>
        <class>DatoClases.EventoClas</class>
        <class>DatoClases.Rutina</class>
        <class>DatoClases.TiempoEntreno</class>
        <class>DatoClases.Tiempo_Competencia</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/CRONOSPORT"/>
            <property name="javax.persistence.jdbc.user" value="root"/>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.password" value=""/>
        </properties>
    </persistence-unit>
</persistence>
